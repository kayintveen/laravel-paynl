{
    "name": "deniztezcan/laravel-paynl",
    "description": "Laravel wrapper for Pay.nl PHP SDK",
    "type": "library",
    "license": "MIT",
    "version": "1.1.0",
    "homepage": "https://github.com/deniztezcan/laravel-paynl",
    "authors": [
        {
            "name": "Deniz Tezcan",
            "email": "deniz@nopulp.nl"
        }
    ],
    "autoload": {
        "psr-4": { "DenizTezcan\\LaravelPayNL\\" : "src/" }
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^8.0.2",
        "guzzlehttp/guzzle": "^6.3|^7.0",
        "illuminate/support": "5.4.*|5.5.*|5.6.*|5.7.*|5.8.*|6.*.*|7.*.*|8.*.*|9.*.*|10.*.*|11.*.*",
        "paynl/sdk": "2.*.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    },
    "extra": {
      "laravel": {
        "providers": [
          "DenizTezcan\\LaravelPayNL\\PayNLServiceProvider"
        ],
        "aliases": {
          "PayNL": "DenizTezcan\\LaravelPayNL\\Facades\\PayNL"
        }
      }
    }
}
